@model IEnumerable<Лабораторная_11.Models.Model2>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Результат 4-го запроса</h2>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            Должность
        </th>
        <th>
            Максимальная зарплата
        </th>
        <th>
            Минимальная зарплата
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Должность
            </td>
            <td>
                @item.Максимальная_зарплата
            </td>
            <td>
                @item.Минимальная_зарплата
            </td>
        </tr>
    }
</table>

